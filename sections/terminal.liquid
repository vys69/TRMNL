{{ 'jquery-3.6.4.min.js' | asset_url | script_tag }}
{{ 'jquery.terminal.min.css' | asset_url | stylesheet_tag }}
{{ 'jquery.terminal.min.js' | asset_url | script_tag }}
<div id="terminal"></div>
<script>
    document.addEventListener('DOMContentLoaded', function() {
        new Terminal('#terminal', {
            greetings: 'Welcome to the jQuery Terminal',
            name: 'js_demo',
            height: 400,
            prompt: 'js> '
        }).echo(function(command, term) {
            if (command !== '') {
                term.echo('You typed: ' + command);
            } else {
                term.echo('');
            }
        });
    });
</script>
{% schema %}    
    {
      "name": "Terminal",
      "class": "TRMNL",
      "tag": "section",
      "settings": [
        
